$.giveBrand.displayNotification('success', 'An invitation message has been sent to <%= @invitation_message.name %>.');

$.giveBrand.invitationMessageQtipApi.hide();
$('.loading').hide();
$("#row_contact_<%=@invitation_message.instance_variable_get('@contact').id %>").addClass('vouch').find('.status').html('');
$("#js-invitation-message-header").text('');
$('#invitation_message_uid').val('');
$('#invitation_message_screen_name').val('');
$('#invitation_message_tag1').val('');
$('#invitation_message_tag2').val('');
$('#invitation_message_tag3').val('');

var inviteButton = $('a[data-screen_name="<%= @invitation_message.screen_name %>"]');
inviteButton.parents('li').addClass('vouch');
inviteButton.remove();
mixpanel.track("Invitation");
