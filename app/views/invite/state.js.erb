$(".js-import-connections").html('<%= j render 'import_providers' %>');
  <% if current_user.twitter_state == 'pending' || current_user.linkedin_state == 'pending' || current_user.facebook_state == 'pending' %>
  var loopInviteAjax = setTimeout(function(){
    clearInterval(loopInviteAjax);
    $.giveBrand.importingCheck();
  }, 4000);
<% else %>
  $("#js-twitter-contacts-list").html('<%=j render 'contacts' %>');
  $("#js-search-contacts").show();
  if($("#user_avatar_main").length > 0){
    $("#user_avatar_main").html('<%= image_tag(current_user.avatar_url(:thumb)) %>');
  }

  if($("#user_avatar_sidebar").length > 0){
    $("#user_avatar_sidebar").html('<%= image_tag(current_user.avatar_url(:avatar)) %>');
  }
<% end %>
$.giveBrand.importConnections();
